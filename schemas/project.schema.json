{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "title",
    "description",
    "cases"
  ],
  "additionalProperties": false,
  "properties": {
    "id": { "type": "string", "title": "Uuid" },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "cases": {
      "type": "array",
      "items": {
        "type": "string",
        "title": "CasePath"
      }
    }
  }
}